#!/usr/local/bin/bash
# Utility to enable/disable Apache Directories/Virtual Hosts for FreeBSD
# marcelbonnet
# 2016-10-10
# with parts from a lost site on the internet about apache and cent os...

ETC=/usr/local/etc/apache24
HTTPD_CONF=$ETC/httpd.conf
DATA=/usr/local/www/apache24/data
DIR_AVAILABLE=$ETC/sites-available
DIR_ENABLED=$ETC/sites-enabled

# gbase64 logo:
FBSD_IMG=""

function make_index(){
    index=$DATA/ABOUTME.html
    admin=$(grep -E "ServerAdmin .*@.*\..*" $HTTPD_CONF | awk '{print $2}')
    all_sites=$(grep Alias $DIR_AVAILABLE/*conf | sort | awk '{print $3}' )
    for site in `grep Alias $DIR_ENABLED/*conf`
    do
        all_sites=$(echo $all_sites | sed "s,$site,," )
    done;

    #sites=$(grep Alias $DIR_AVAILABLE/*conf | sort | awk '{print "<li><a href="$3">"$3"</a></li>"}'  | tr "\n" " " )
    sites=$( for site in `echo $all_sites | sort`; do echo $site | awk '{print "<li><a href=#>"$1"</a></li>"}' ; done  | tr "\n" " " )
    ensites=$(grep Alias $DIR_ENABLED/*conf | sort | awk '{print "<li><a href="$3">"$3"</a></li>"}'  | tr "\n" " " )

    # FIXME: gera duas linhas repetidas com todos os links de sites
    #sed -i.bkp "s,<!--AV_START-->,$sites," $index
    #sed -i.bkp "/<!--AV_START-->/,/<!--AV_END-->/ s,.*,<!--AV_START-->$sites<!--AV_END-->," $index

    cat > $DATA/index.html << _EOF_
<!DOCTYPE html>
<HTML>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
<style>
    a:-webkit-any-link {
        color: #0E65BB;
        text-decoration: underline;
        cursor: auto;
    }
    #available a:-webkit-any-link {
        color: #7F7F7F;
        text-decoration: underline;
        cursor: auto;
    }
</style>
<title>Apache Services</title>
</head>
<body>
<h1>`uname -rsn`</h1>

<div style="padding-left:20%">
    <div id="enabled" style="float:left; width:40%">
        <h2>Enabled Sites</h2>
        $ensites
    </div>
    <div id="available" style="float:left; width:40%">
        <h2>Available Sites</h2>
        $sites
    </div>
</div>

<div style="clear:both">
    <hr>
    <small>
        <div style="text-align: center">
            <p><strong>Powered by FreeBSD | $admin</strong></p>
            <img src="data:image/jpg;base64,$FBSD_IMG">
        </div>
    </small>
</div>
</body>
</HTML>
_EOF_

echo "Apache index.html updated."
}

function disable(){
    avail=$DIR_ENABLED/$1.conf
    enabled=$DIR_ENABLED
    site=`ls $DIR_ENABLED/`
    if test -e $avail; then
        sudo rm  $avail
    else
        echo -e "$avail virtual host does not exist! Exiting!"
        exit 1
    fi

    if test -e $enabled/$1.conf; then
        echo "Error!! Could not remove $avail virtual host!"
        exit 1
    else
        echo  -e "$avail has been removed!\nRestart Apache server."
    fi
}

function enable(){
    if test -d $DIR_AVAILABLE && test -d $DIR_ENABLED  ; then
        echo "-----------------------------------------------"
    else
        mkdir $DIR_AVAILABLE
        mkdir $DIR_ENABLED
    fi

    avail=$DIR_AVAILABLE/$1.conf
    enabled=$DIR_ENABLED/
    site=`ls $DIR_AVAILABLE/`

    if test -e $avail; then
        sudo ln -s $avail $enabled
    else
        echo -e "$avail virtual host does not exist! Please create one!\n$site"
        exit 1
    fi

    if test -e $enabled/$1.conf; then
        echo "Success. Now restart Apache server."
    else
        echo  -e "Virtual host $avail does not exist!\nPlease see available virtual hosts:\n$site"
        exit 1
    fi
}


function usage(){
    cat << _EOF_
`basename $0` [ -e | -d | -i | -h ]
    -e  [file_name].conf enable site (vhost, directory)
    -d  [file_name].conf disable site (vhost, directory)
    -i  make index of available/enabled sites (DocumentRoot/index.html)
    -h  show this message

   author: https://github.com/marcelbonnet/darth-script
_EOF_
}

# Options
while [ "${1#-}" != "$1" ]
do
        case "$1" in
                -e) shift;
                        enable $1
                        make_index
                        sudo service apache24 restart
                        exit 0
                        ;;
                -d) shift;
                        disable $1
                        make_index
                        sudo service apache24 restart
                        exit 0
                        ;;
                -i) shift;
                        make_index
                        exit 0
                        ;;
                -h) usage;        exit 0;;
                *)  usage; break       ;;
        esac
 shift
done
